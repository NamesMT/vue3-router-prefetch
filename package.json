{
  "name": "vue3-router-prefetch",
  "version": "1.2.0",
  "type": "module",
  "main": "lib.umd.cjs",
  "module": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cypress:open": "cypress open",
    "publish": "vue-tsc && vite build && np --no-tests"
  },
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "require": "./dist/lib.umd.cjs",
      "import": "./dist/lib.js"
    },
    "./*": "./*"
  },
  "dependencies": {
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "peerDependencies": {
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@types/node": "^18.14.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "cypress": "^12.7.0",
    "eslint": "^8.34.0",
    "np": "^7.6.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.2",
    "vue-tsc": "^1.0.24"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
